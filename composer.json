{
    "name": "andreiavrammsd/serializer",
    "type": "library",
    "description": "JSON string to object. Object/array to JSON.",
    "keywords": ["json", "serializer", "unserializer", "transformer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Avram",
            "email": "avramandrei@ymail.com",
            "homepage": "https://www.andreiavram.ro/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11",
        "phpmd/phpmd": "^2.8",
        "overtrue/phplint": "^1.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4" : {
            "Serializer\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serializer\\Tests\\": "tests/"
        }
    }
}
